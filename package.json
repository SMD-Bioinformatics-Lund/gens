    "@types/gulp-uglify": "^3.0.11",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.14.0",
    "@types/webpack": "^5.28.5",
    "css-loader": "^7.1.2",
    "eslint": "^9.24.0",
    "globals": "^16.0.0",
    "gulp": "^5.0.0",
